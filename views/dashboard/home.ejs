  <style>
    th[class="w-[1000px]"] {
      min-width: 500px;
    }

    .drop-area {
      border: 2px dashed #ccc;
      height: 50px;
      margin-top: 10px;
    }
    .item.dragging :where(.details, i) {
      opacity: 0;
    }
    .hoverDash {
      border-radius: 10px;
      border-style: dashed;
      border-width: 2px; /* Adjust the width of the border */
      border-spacing: 6px; /* Adjust the spacing between dashes */
    }

    .draggable-container {
      transition: transform 0.3s ease-in-out;
    }

    .highlight {
      background-color: #f0f9ff !important; /* Light blue color for highlighting */
    }
  </style>

  <body
    x-data="{ activeItem: 'Dashboard', showModalLoading:false, bkupfile:0 }"
    class="has-min-sidebar is-sidebar-open is-header-blur"
    x-bind="$store.global.documentBody"
  >
    <!-- App preloader-->
    <div
      class="app-preloader fixed z-50 grid h-full w-full place-content-center bg-slate-50 dark:bg-navy-900"
    >
      <div class="app-preloader-inner relative inline-block h-48 w-48"></div>
    </div>

    <!-- Page Wrapper -->
    <div
      id="root"
      class="min-h-100vh flex grow bg-slate-50 dark:bg-navy-900"
      x-cloak
    >
      <div><%- include('../includes/dash/loading.ejs') %></div>
      <!-- Main Sidebar -->
      <div class="main-sidebar">
        <div class="flex h-full grow flex-col bg-white dark:bg-navy-750">
          <!-- Sidebar Panel Header -->
          <div class="flex h-18 w-full items-center justify-between pl-4 pr-1">
