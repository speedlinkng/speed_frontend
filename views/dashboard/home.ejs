  <style>
    th[class="w-[1000px]"] {
      min-width: 500px;
    }

    .drop-area {
      border: 2px dashed #ccc;
      height: 50px;
      margin-top: 10px;
    }
    .item.dragging :where(.details, i) {
      opacity: 0;
    }
    .hoverDash {
      border-radius: 10px;
      border-style: dashed;
      border-width: 2px; /* Adjust the width of the border */
      border-spacing: 6px; /* Adjust the spacing between dashes */
    }

    .draggable-container {
      transition: transform 0.3s ease-in-out;
    }
  </style>

  <body
    x-data="{ activeItem: 'Dashboard', showModalLoading:false, bkupfile:0 }"
    class="has-min-sidebar is-sidebar-open is-header-blur"
    x-bind="$store.global.documentBody"
  >
    <!-- App preloader-->

    <!-- Page Wrapper -->
    <div
      id="root"
      class="min-h-100vh flex grow bg-slate-50 dark:bg-navy-900"
      x-cloak
    >
      <div><%- include('../includes/dash/loading.ejs') %></div>
      <!-- Main Sidebar -->
      <div class="main-sidebar">
        <div class="flex h-full grow flex-col bg-white dark:bg-navy-750">
          <!-- Sidebar Panel Header -->
          <div class="flex h-18 w-full items-center justify-between pl-4 pr-1">
            <div
              class="md:col-span-1 col-span-3 text-left md:pl-10 flex items-center"
            >
              LOGO
            </div>
            <button
              @click="$store.global.isSidebarExpanded = false"
              class="btn h-7 w-7 rounded-full p-0 text-primary lg:hidden hover:bg-slate-300/20 focus:bg-slate-300/20 active:bg-slate-300/25 dark:text-accent-light/80 dark:hover:bg-navy-300/20 dark:focus:bg-navy-300/20 dark:active:bg-navy-300/25"
            >
              <svg
                xmlns="http://www.w3.org/2000/svg"
                class="h-6 w-6"
                fill="none"
                viewbox="0 0 24 24"
                stroke="currentColor"
              >
                <path
                  stroke-linecap="round"
                  stroke-linejoin="round"
                  stroke-width="2"
                  d="M15 19l-7-7 7-7"
                />
              </svg>
            </button>
          </div>

          <!-- Sidebar Panel Body -->
          <div x-data class="flex h-[calc(100%-4.5rem)] grow flex-col">
            <div
              x-data="{showModal2:false}"
              class="is-scrollbar-hidden grow overflow-y-auto"
            >
              <div
                class="my-[1px] mx-4 h-px bg-slate-200 dark:bg-navy-500"
              ></div>

              <ul class="space-y-1.5 px-2 pt-5 font-inter text-md font-medium">
                <li id="" class="w-full pr-4 mb-2 font-normal">
                  <div
                    class="flex space-x-3 rounded-lg bg-slate-100 p-3 dark:bg-navy-500"
                  >
                    <div class="avatar border flex items-center justify-center">
                      <svg
                        xmlns="http://www.w3.org/2000/svg"
                        width="16"
                        height="16"
                        fill="currentColor"
                        class="bi bi-person-circle w-18 h-18"
                        viewBox="0 0 16 16"
                      >
                        <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0" />
                        <path
                          fill-rule="evenodd"
                          d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8m8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1"
                        />
                      </svg>
                    </div>
                    <div>
                      <p class="text-xs text-slate-500 dark:text-navy-200">
                        Welcome ...
                      </p>
                      <p
                        class="font-medium text-slate-700 dark:text-navy-100 capitalize"
                      >
                        <%= data.firstname %> <%= data.lastname %>
                      </p>
                    </div>
                  </div>
                </li>
                <li class="h-2"></li>
                <li
                  id="_Dashboard"
                  @click="activeItem = 'Dashboard'; $store.global.isSidebarExpanded = false"
                >
                  <a
                    onclick=" localStorage.setItem('activeItem', 'Dashboard'), localStorage.setItem('activeItemTimestamp', Date.now());"
                    @click="activeItem = 'Dashboard'"
                    :class="[activeItem === 'Dashboard' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light' : '']"
                    class="duration-200 cursor-pointer group flex space-x-2 rounded-md p-2 tracking-wide text-slate-800 outline-none transition-all hover:bg-primary/20 focus:bg-primary/20 active:bg-primary/25 dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      fill="currentColor"
                      class="bi bi-house w-7 h-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M8.707 1.5a1 1 0 0 0-1.414 0L.646 8.146a.5.5 0 0 0 .708.708L2 8.207V13.5A1.5 1.5 0 0 0 3.5 15h9a1.5 1.5 0 0 0 1.5-1.5V8.207l.646.647a.5.5 0 0 0 .708-.708L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293zM13 7.207V13.5a.5.5 0 0 1-.5.5h-9a.5.5 0 0 1-.5-.5V7.207l5-5z"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Dashboard' ? 'text-primary dark:text-accent-light' : '']"
                      class="dark:text-navy-100 mt-[2px]"
                      >My Requests</span
                    >
                  </a>
                </li>

                <li
                  id="_Plan"
                  @click="activeItem = 'Plan'; $store.global.isSidebarExpanded = false"
                >
                  <a
                    onclick=" localStorage.setItem('activeItem', 'Plan'), localStorage.setItem('activeItemTimestamp', Date.now());"
                    :class="[activeItem === 'Plan' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light' : '']"
                    class="cursor-pointer group flex space-x-2 rounded-md p-2 tracking-wide text-slate-800 outline-none transition-all hover:bg-primary/20 focus:bg-primary/20 active:bg-primary/25 dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      fill="currentColor"
                      class="bi bi-substack w-7 h-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M15 3.604H1v1.891h14v-1.89ZM1 7.208V16l7-3.926L15 16V7.208zM15 0H1v1.89h14z"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Plan' ? 'text-primary dark:text-accent-light' : '']"
                      class="dark:text-navy-100 mt-[2px]"
                      >Plan</span
                    >
                  </a>
                </li>
                <li
                  id="_Zoom"
                  class="relative"
                  @click="activeItem = 'Zoom'; $store.global.isSidebarExpanded = false"
                >
                  <a
                    :class="[activeItem === 'Zoom' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light ' : '']"
                    onclick=" refresh('refresh'),localStorage.setItem('activeItem', 'Zoom'), localStorage.setItem('activeItemTimestamp', Date.now());"
                    class="cursor-pointer group flex space-x-2 rounded-md p-2 tracking-wide text-slate-800 hover:bg-primary/20 focus:bg-primary/20 active:bg-primary/25 outline-none transition-all dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      fill="currentColor"
                      class="bi bi-camera-video h-7 w-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        fill-rule="evenodd"
                        d="M0 5a2 2 0 0 1 2-2h7.5a2 2 0 0 1 1.983 1.738l3.11-1.382A1 1 0 0 1 16 4.269v7.462a1 1 0 0 1-1.406.913l-3.111-1.382A2 2 0 0 1 9.5 13H2a2 2 0 0 1-2-2zm11.5 5.175 3.5 1.556V4.269l-3.5 1.556zM2 4a1 1 0 0 0-1 1v6a1 1 0 0 0 1 1h7.5a1 1 0 0 0 1-1V5a1 1 0 0 0-1-1z"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Zoom' ? 'text-primary dark:text-accent-light' : '']"
                      class="dark:text-navy-100"
                      >Zoom Recordings</span
                    >
                    <span
                      class="absolute top-1 right-5 bg-purple-700 font-semibold poppin text-[7px] h-[16px] flex justify-center items-center text-white px-[2px] rounded border"
                    >
                      Plus
                    </span>
                  </a>
                </li>
                <li
                  id="_Settings"
                  @click="activeItem = 'Settings'; $store.global.isSidebarExpanded = false"
                >
                  <a
                    :class="[activeItem === 'Settings' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light' : '']"
                    onclick=" localStorage.setItem('activeItem', 'Settings'), localStorage.setItem('activeItemTimestamp', Date.now());"
                    class="cursor-pointer group flex space-x-2 rounded-md p-2 tracking-wide text-slate-800 outline-none transition-all hover:bg-primary/20 focus:bg-primary/20 active:bg-primary/25 dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="16"
                      height="16"
                      fill="currentColor"
                      class="bi bi-gear h-7 w-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M8 4.754a3.246 3.246 0 1 0 0 6.492 3.246 3.246 0 0 0 0-6.492M5.754 8a2.246 2.246 0 1 1 4.492 0 2.246 2.246 0 0 1-4.492 0"
                      />
                      <path
                        d="M9.796 1.343c-.527-1.79-3.065-1.79-3.592 0l-.094.319a.873.873 0 0 1-1.255.52l-.292-.16c-1.64-.892-3.433.902-2.54 2.541l.159.292a.873.873 0 0 1-.52 1.255l-.319.094c-1.79.527-1.79 3.065 0 3.592l.319.094a.873.873 0 0 1 .52 1.255l-.16.292c-.892 1.64.901 3.434 2.541 2.54l.292-.159a.873.873 0 0 1 1.255.52l.094.319c.527 1.79 3.065 1.79 3.592 0l.094-.319a.873.873 0 0 1 1.255-.52l.292.16c1.64.893 3.434-.902 2.54-2.541l-.159-.292a.873.873 0 0 1 .52-1.255l.319-.094c1.79-.527 1.79-3.065 0-3.592l-.319-.094a.873.873 0 0 1-.52-1.255l.16-.292c.893-1.64-.902-3.433-2.541-2.54l-.292.159a.873.873 0 0 1-1.255-.52zm-2.633.283c.246-.835 1.428-.835 1.674 0l.094.319a1.873 1.873 0 0 0 2.693 1.115l.291-.16c.764-.415 1.6.42 1.184 1.185l-.159.292a1.873 1.873 0 0 0 1.116 2.692l.318.094c.835.246.835 1.428 0 1.674l-.319.094a1.873 1.873 0 0 0-1.115 2.693l.16.291c.415.764-.42 1.6-1.185 1.184l-.291-.159a1.873 1.873 0 0 0-2.693 1.116l-.094.318c-.246.835-1.428.835-1.674 0l-.094-.319a1.873 1.873 0 0 0-2.692-1.115l-.292.16c-.764.415-1.6-.42-1.184-1.185l.159-.291A1.873 1.873 0 0 0 1.945 8.93l-.319-.094c-.835-.246-.835-1.428 0-1.674l.319-.094A1.873 1.873 0 0 0 3.06 4.377l-.16-.292c-.415-.764.42-1.6 1.185-1.184l.292.159a1.873 1.873 0 0 0 2.692-1.115z"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Settings' ? 'text-primary dark:text-accent-light ' : '']"
                      class="text-slate-800 dark:text-navy-100"
                    >
                      Settings
                    </span>
                  </a>
                </li>

                <% if(role == 'admin'){ %>
                <li
                  id="Admin"
                  onclick="goAdmin()"
                  @click=" $store.global.isSidebarExpanded = false"
                >
                  <a
                    :class="[activeItem === 'Admin' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light ' : '']"
                    class="cursor-pointer group flex space-x-2 rounded-lg p-2 tracking-wide text-slate-800 outline-none transition-all hover:bg-primary/20 focus:bg-primary/20 active:bg-primary/25 dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="16"
                      height="16"
                      fill="currentColor"
                      class="bi bi-person-gear h-7 w-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M11 5a3 3 0 1 1-6 0 3 3 0 0 1 6 0M8 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4m.256 7a4.5 4.5 0 0 1-.229-1.004H3c.001-.246.154-.986.832-1.664C4.484 10.68 5.711 10 8 10q.39 0 .74.025c.226-.341.496-.65.804-.918Q8.844 9.002 8 9c-5 0-6 3-6 4s1 1 1 1zm3.63-4.54c.18-.613 1.048-.613 1.229 0l.043.148a.64.64 0 0 0 .921.382l.136-.074c.561-.306 1.175.308.87.869l-.075.136a.64.64 0 0 0 .382.92l.149.045c.612.18.612 1.048 0 1.229l-.15.043a.64.64 0 0 0-.38.921l.074.136c.305.561-.309 1.175-.87.87l-.136-.075a.64.64 0 0 0-.92.382l-.045.149c-.18.612-1.048.612-1.229 0l-.043-.15a.64.64 0 0 0-.921-.38l-.136.074c-.561.305-1.175-.309-.87-.87l.075-.136a.64.64 0 0 0-.382-.92l-.148-.045c-.613-.18-.613-1.048 0-1.229l.148-.043a.64.64 0 0 0 .382-.921l-.074-.136c-.306-.561.308-1.175.869-.87l.136.075a.64.64 0 0 0 .92-.382zM14 12.5a1.5 1.5 0 1 0-3 0 1.5 1.5 0 0 0 3 0"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Settings' ? 'text-primary dark:text-accent-light ' : '']"
                      class="text-slate-800 dark:text-navy-100"
                    >
                      Admin
                    </span>
                  </a>
                </li>

                <% }%>

                <br />
                <br />
                <li
                  id="_Logout"
                  class="absolute bottom-1 w-full pr-4 font-normal"
                >
                  <a
                    @click="showModal2 = true"
                    :class="[activeItem === 'Logout' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light ' : '']"
                    class="w-[100%] relative cursor-pointer group flex space-x-2 rounded-lg p-2 tracking-wide text-red-500 outline-none transition-all dark:text-red-500 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="16"
                      height="16"
                      fill="currentColor"
                      class="bi bi-exclamation-octagon h-7 w-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        d="M4.54.146A.5.5 0 0 1 4.893 0h6.214a.5.5 0 0 1 .353.146l4.394 4.394a.5.5 0 0 1 .146.353v6.214a.5.5 0 0 1-.146.353l-4.394 4.394a.5.5 0 0 1-.353.146H4.893a.5.5 0 0 1-.353-.146L.146 11.46A.5.5 0 0 1 0 11.107V4.893a.5.5 0 0 1 .146-.353zM5.1 1 1 5.1v5.8L5.1 15h5.8l4.1-4.1V5.1L10.9 1z"
                      />
                      <path
                        d="M7.002 11a1 1 0 1 1 2 0 1 1 0 0 1-2 0M7.1 4.995a.905.905 0 1 1 1.8 0l-.35 3.507a.552.552 0 0 1-1.1 0z"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Logout' ? 'text-error dark:text-error ' : '']"
                      class="text-error dark:text-error"
                    >
                      Logout
                    </span>
                  </a>
                  <div id="logout">
                    <%- include('../includes/dash/logout.ejs') %>
                  </div>
                </li>

                <li id="_Create" @click="activeItem = 'Create'" class="hidden">
                  create
                </li>

                <li
                  id="_Share"
                  @click="activeItem = 'Share'"
