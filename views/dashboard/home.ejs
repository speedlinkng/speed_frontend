      <!-- Main Content Wrapper -->
      <main x-data class="main-content filemanager-app w-full pb-6">
        <div><%- include('../includes/notifications.ejs') %></div>

        <div id="dashoard" x-show="activeItem === 'Dashboard'">
          <%- include('../includes/dash/dashboard.ejs') %>
        </div>
        <div id="submissions" x-show="activeItem === 'Submissions'">
          <%- include('../includes/dash/submissions.ejs') %>
        </div>
        <div id="plan" x-show="activeItem === 'Plan'">
          <%- include('../includes/dash/plan.ejs') %>
        </div>
        <div id="zoomInt" x-show="activeItem === 'Zoom'">
          <%- include('../includes/zoom/zoom.ejs') %>
        </div>
        <div
