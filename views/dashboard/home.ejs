                  <a
                    :class="[activeItem === 'Zoom' ? 'bg-side text-primary dark:text-accent-light hover:bg-side' : '']"
                    class="border-b duration-200 cursor-pointer group flex items-center space-x-2 rounded-md p-2 tracking-wide text-slate-800 outline-none transition-all hover:bg-[#CDEED3] hover:text-primary focus:bg-side active:bg-side dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                    onclick=" refresh('refresh'),localStorage.setItem('activeItem', 'Zoom'), localStorage.setItem('activeItemTimestamp', Date.now());"
                  >
                    <div class="flex p-1 w-7 h-7 justify-center items-center">
                      <i
                        class="text-xl"
                        :class="activeItem === 'Zoom' ? 'bi bi-camera-video-fill text-primary': 'bi bi-camera-video '"
