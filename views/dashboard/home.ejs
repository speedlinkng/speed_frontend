                    class="border-b cursor-pointer group flex items-center space-x-2 rounded-lg p-2 tracking-wide text-slate-800 outline-none transition-all hover:bg-primary/20 hover:text-primary focus:bg-primary/20 active:bg-primary/25 dark:text-navy-100 dark:hover:bg-navy-600 dark:focus:bg-navy-600"
                  >
                    <div class="flex p-1 w-7 h-7 justify-center items-center">
                      <i
                        class="text-xl"
                        :class="activeItem === 'Admin' ? 'bi bi-person-fill-gear text-primary': 'bi bi-person-gear'"
                      ></i>
                      <svg
                        xmlns="http://www.w3.org/2000/svg"
                        width="16"
                        height="16"
                        fill="currentColor"
                        class="bi bi-person-gear h-7 w-7 icon-outline"
                        viewBox="0 0 16 16"
                      >
                        <path
                          d="M11 5a3 3 0 1 1-6 0 3 3 0 0 1 6 0M8 7a2 2 0 1 0 0-4 2 2 0 0 0 0 4m.256 7a4.5 4.5 0 0 1-.229-1.004H3c.001-.246.154-.986.832-1.664C4.484 10.68 5.711 10 8 10q.39 0 .74.025c.226-.341.496-.65.804-.918Q8.844 9.002 8 9c-5 0-6 3-6 4s1 1 1 1zm3.63-4.54c.18-.613 1.048-.613 1.229 0l.043.148a.64.64 0 0 0 .921.382l.136-.074c.561-.306 1.175.308.87.869l-.075.136a.64.64 0 0 0 .382.92l.149.045c.612.18.612 1.048 0 1.229l-.15.043a.64.64 0 0 0-.38.921l.074.136c.305.561-.309 1.175-.87.87l-.136-.075a.64.64 0 0 0-.92.382l-.045.149c-.18.612-1.048.612-1.229 0l-.043-.15a.64.64 0 0 0-.921-.38l-.136.074c-.561.305-1.175-.309-.87-.87l.075-.136a.64.64 0 0 0-.382-.92l-.148-.045c-.613-.18-.613-1.048 0-1.229l.148-.043a.64.64 0 0 0 .382-.921l-.074-.136c-.306-.561.308-1.175.869-.87l.136.075a.64.64 0 0 0 .92-.382zM14 12.5a1.5 1.5 0 1 0-3 0 1.5 1.5 0 0 0 3 0"
                        />
                      </svg>

                      <svg
                        xmlns="http://www.w3.org/2000/svg"
                        width="16"
                        height="16"
                        fill="currentColor"
                        class="bi bi-person-fill-gear w-7 h-7 icon-filled"
                        viewBox="0 0 16 16"
                      >
                        <path
                          d="M11 5a3 3 0 1 1-6 0 3 3 0 0 1 6 0m-9 8c0 1 1 1 1 1h5.256A4.5 4.5 0 0 1 8 12.5a4.5 4.5 0 0 1 1.544-3.393Q8.844 9.002 8 9c-5 0-6 3-6 4m9.886-3.54c.18-.613 1.048-.613 1.229 0l.043.148a.64.64 0 0 0 .921.382l.136-.074c.561-.306 1.175.308.87.869l-.075.136a.64.64 0 0 0 .382.92l.149.045c.612.18.612 1.048 0 1.229l-.15.043a.64.64 0 0 0-.38.921l.074.136c.305.561-.309 1.175-.87.87l-.136-.075a.64.64 0 0 0-.92.382l-.045.149c-.18.612-1.048.612-1.229 0l-.043-.15a.64.64 0 0 0-.921-.38l-.136.074c-.561.305-1.175-.309-.87-.87l.075-.136a.64.64 0 0 0-.382-.92l-.148-.045c-.613-.18-.613-1.048 0-1.229l.148-.043a.64.64 0 0 0 .382-.921l-.074-.136c-.306-.561.308-1.175.869-.87l.136.075a.64.64 0 0 0 .92-.382zM14 12.5a1.5 1.5 0 1 0-3 0 1.5 1.5 0 0 0 3 0"
                        />
                      </svg>
                    </div>

                    <span
                      :class="[activeItem === 'Settings' ? 'text-primary dark:text-accent-light ' : '']"
                      class="text-slate-800 dark:text-navy-100"
                    >
                      Admin
                    </span>
                  </a>
                </li>

                <% }%>

                <br />
                <br />
                <li
                  id="_Logout"
                  class="absolute bottom-1 w-full pr-4 font-normal"
                >
                  <a
                    @click="showModal2 = true"
                    :class="[activeItem === 'Logout' ? 'bg-[rgba(79,70,229,.1)] text-primary dark:text-accent-light ' : '']"
                    class="w-[100%] relative cursor-pointer group flex space-x-2 rounded-lg p-2 tracking-wide text-red-500 outline-none transition-all dark:text-red-500 dark:hover:bg-navy-600 hover:text-error dark:focus:bg-navy-600"
                  >
                    <svg
                      xmlns="http://www.w3.org/2000/svg"
                      width="16"
                      height="16"
                      fill="currentColor"
                      class="bi bi-box-arrow-right h-7 w-7 pr-2 -mt-[2.5px]"
                      viewBox="0 0 16 16"
                    >
                      <path
                        fill-rule="evenodd"
                        d="M10 12.5a.5.5 0 0 1-.5.5h-8a.5.5 0 0 1-.5-.5v-9a.5.5 0 0 1 .5-.5h8a.5.5 0 0 1 .5.5v2a.5.5 0 0 0 1 0v-2A1.5 1.5 0 0 0 9.5 2h-8A1.5 1.5 0 0 0 0 3.5v9A1.5 1.5 0 0 0 1.5 14h8a1.5 1.5 0 0 0 1.5-1.5v-2a.5.5 0 0 0-1 0z"
                      />
                      <path
                        fill-rule="evenodd"
                        d="M15.854 8.354a.5.5 0 0 0 0-.708l-3-3a.5.5 0 0 0-.708.708L14.293 7.5H5.5a.5.5 0 0 0 0 1h8.793l-2.147 2.146a.5.5 0 0 0 .708.708z"
                      />
                    </svg>

                    <span
                      :class="[activeItem === 'Logout' ? 'text-error dark:text-error ' : '']"
                      class="text-error dark:text-error"
                    >
                      Logout
                    </span>
                  </a>
                  <div id="logout">
                    <%- include('../includes/dash/logout.ejs') %>
                  </div>
                </li>

                <li id="_Create" @click="activeItem = 'Create'" class="hidden">
                  create
                </li>

                <li
                  id="_Share"
                  @click="activeItem = 'Share'"
                  class="hidden"
                ></li>
              </ul>
            </div>
          </div>
        </div>
      </div>

      <!-- App Navigation Wrapper-->
      <nav class="header dark:bg-slate-500">
        <div x-data class="header-container grid grid-cols-10 w-full -px-5">
          <div
            class="col-span-10 flex justify-start items-center space-x-2 px-[var(--margin-x)] w-full transition-all duration-[.25s]"
          >
            <!-- Left: Sidebar Toggle Button -->
            <div class="h-7 w-7 md:hidden">
              <button
                class="md:hidden menu-toggle ml-0.5 flex h-7 w-7 flex-col justify-center space-y-1.5 text-primary outline-none focus:outline-none dark:text-accent-light/80"
                :class="$store.global.isSidebarExpanded && ''"
                @click="$store.global.isSidebarExpanded = !$store.global.isSidebarExpanded"
              >
                <span></span>
                <span></span>
                <span></span>
              </button>
            </div>
            <div class="flex items-center space-x-3 py-3 lg:py-3">
              <ul class="flex flex-wrap items-center space-x-2 text-base">
                <li
                  @click="activeItem = 'Dashboard'"
                  class="flex items-center space-x-2"
                >
                  <a
                    @click="activeItem = 'Dashboard'"
                    :class="[activeItem === 'Dashboard' ? 'text-primary text-base transition-colors hover:text-primary-focus dark:text-accent-light dark:hover:text-accent' : '']"
                    href="#"
                    >Dashboard</a
                  >
                  <span class="px-2">|</span>
                </li>
                <li
                  @click="activeItem = 'Submissions'"
                  :class="{ 'hidden': activeItem !== 'Submissions' }"
                >
                  <a
                    :class="{ 'text-primary text-base transition-colors hover:text-primary-focus dark:text-accent-light dark:hover:text-accent': activeItem === 'Plan' }"
                    class="cursor-pointer"
                    href="#"
                    >Submissions</a
                  >
                  <span class="px-2 showAdmin">|</span>
                  <span
                    class="submission_for text-primary dark:text-accent-light"
                  ></span>
                </li>
                <li
                  @click="activeItem = 'Submissions'"
                  :class="{ 'hidden': activeItem !== 'Plan' }"
                >
                  <a
                    :class="{ 'text-primary text-base transition-colors hover:text-primary-focus dark:text-accent-light dark:hover:text-accent': activeItem === 'Plan' }"
                    class="cursor-pointer"
                    href="#"
                    >Plan</a
                  >
                  <span class="px-2 showAdmin">|</span>
                </li>
                <li
                  @click="activeItem = 'Create'"
                  :class="{ 'hidden': activeItem !== 'Create' }"
                >
                  <a
                    :class="[activeItem === 'Create' ? 'text-primary text-base transition-colors hover:text-primary-focus dark:text-accent-light dark:hover:text-accent' : '']"
                    class="cursor-pointer"
                    href="#"
                    >Create</a
                  >
                  <span class="px-2 showAdmin">- </span
