                        <path d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0" />
                        <path
                          fill-rule="evenodd"
                          d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8m8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1"
                        />
                      </svg>
                    </div>
                    <div>
                      <p class="text-xs dark:text-navy-200">Welcome ...</p>
                      <p class="font-medium dark:text-navy-100 capitalize">
                        <%= data.firstname %> <%= data.lastname %>
                      </p>
                    </div>
                  </div>
                </li>
                <li class="h-2"></li>
                <% if(role == 'user'){ %>
                <li
                  id="_Dashboard"
                  @click="activeItem = 'Dashboard'; $store.global.isSidebarExpanded = false"
                >
                  <a
                    onclick=" localStorage.setItem('activeItem', 'Dashboard'), localStorage.setItem('activeItemTimestamp', Date.now());"
