                    <div class="flex p-1 w-7 h-7 justify-center items-center">
                      <i
                        class="text-xl"
                        :class="activeItem === 'Settings' ? 'bi bi-gear-fill text-primary': 'bi bi-gear'"
                      ></i>
                    </div>

                    <span
                      :class="[activeItem === 'Settings' ? 'text-primary dark:text-accent-light ' : '']"
                      class="dark:text-navy-100"
                    >
                      Settings
                    </span>
                  </a>
                </li>
                <% } %> <% if(role == 'admin'){ %>
